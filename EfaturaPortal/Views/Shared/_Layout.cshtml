
<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE 3 | Starter</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/plugins/adminLte/plugins/fontawesome-free/css/all.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/plugins/adminLte/dist/css/adminlte.min.css">
    <!--Datatable Css-->
    <link href="~/plugins/adminLte/plugins/datatables-bs4/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/plugins/adminLte/plugins/datatables-responsive/css/responsive.bootstrap4.css" rel="stylesheet" />
    <link href="~/plugins/adminLte/plugins/datatables-buttons/css/buttons.bootstrap4.css" rel="stylesheet" />
    <link href="~/plugins/adminLte/plugins/select2/css/select2.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/plugins/adminLte/plugins/select2-bootstrap4-theme/select2-bootstrap4.css" rel="stylesheet" asp-append-version="true"  />
    @*<link href="~/plugins/datatables/datatables.bundle.css" rel="stylesheet" />*@
    <!--Notiflix Css-->
    <link href="~/plugins/notiflix/css/notiflix.css" rel="stylesheet" />

    <link href="~/css/site.css" rel="stylesheet" asp-append-version="true" />


    @RenderSection("css", false)
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">

        <!-- Navbar -->
        <partial name="_Header" />
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <partial name="_SideBar" />
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="content">

                <div class="container-fluid pt-3">
                    @RenderBody()
                </div>
            </div>
        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
            <div class="p-3">
                <h5>Title</h5>
                <p>Sidebar content</p>
            </div>
        </aside>
        <!-- /.control-sidebar -->
        <!-- Main Footer -->
        <partial name="_Footer" />
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="~/plugins/adminLte/plugins/jquery/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.ajax.js"></script>
    <!-- Bootstrap 4 -->
    <script src="~/plugins/adminLte/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/plugins/adminLte/dist/js/adminlte.min.js"></script>
    <!--DataTable Js-->
    <script src="~/plugins/adminLte/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
    <script src="~/plugins/adminLte/plugins/jszip/jszip.min.js"></script>
    <script src="~/plugins/adminLte/plugins/pdfmake/pdfmake.min.js"></script>
    <script src="~/plugins/adminLte/plugins/pdfmake/vfs_fonts.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
    <script src="~/plugins/adminLte/plugins/datatable-moment/moment.min.js"></script>
    <script src="~/plugins/adminLte/plugins/select2/js/select2.full.min.js" asp-append-version="true" ></script>
    <script src="~/plugins/adminLte/plugins/select2/js/i18n/tr.js"></script>

    @*<script src="~/plugins/datatables/datatables.bundle.js" asp-append-version="true"></script>*@
    <!--Notiflix-->
    <script src="~/plugins/notiflix/js/notiflix.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>


    @RenderSection("Scripts", false)
</body>
</html>